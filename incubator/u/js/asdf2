    {{#each data}}
        <li class="vote {{#if is_created_by_instructor}}by-ins{{/if}}" data-answerid="{{id}}">
            <div class="vote">
                <div class="btns {{#if is_owner}}none{{/if}}">
                    {{#if user_vote}}
                        {{#if upvoted}}
                        <a class="up on" href="#" data-answerId="{{id}}"></a>
                        <a class="down" href="#" data-answerId="{{id}}"></a>
                        {{else}}
                        <a class="up" href="#" data-answerId="{{id}}"></a>
                        <a class="down on" href="#" data-answerId="{{id}}"></a>
                        {{/if}}
                    {{else}}
                        <a class="up" href="#" data-answerId="{{id}}"></a>
                        <a class="down" href="#" data-answerId="{{id}}"></a>
                    {{/if}}
                </div>
                <span title="There are {{vote_count}} votes">{{vote_count}}</span>
            </div>
