                        <a href="#" data-userid="{{id}}" class="following btn btn-success btn-small">Following</a>
                    {{/is_following}}
                    {{^is_following}}
                        <a href="#" data-userid="{{id}}" class="follow btn btn-primary btn-small">Follow</a>
                    {{/is_following}}

                    <a href="{{generateMessageUrl id}}"
                       class="ud-popup message btn btn-small">Message</a>
                {{/show_buttons}}
            </span>
        </li>
    {{/each}}

</script>  
            </div>

        </div>
    </div>  
    
